{"nbformat":4,"nbformat_minor":0,"metadata":{"colab":{"provenance":[]},"kernelspec":{"name":"python3","display_name":"Python 3"},"language_info":{"name":"python"}},"cells":[{"cell_type":"code","execution_count":null,"metadata":{"id":"MXxCTsc6RTF8"},"outputs":[],"source":["# Implements Eulers method to solve a first order ODE of the form dy/dt = f(t,y)\n","\n","# import packages\n","import matplotlib.pyplot as plt\n","import numpy as np\n","\n","plt.rcParams['font.size'] = '16'"]},{"cell_type":"code","source":["\n","# Define Euler's method - we want to save vectors of t and y values\n","#import numpy as np\n","def euler1(t0,t1,y,n):\n","  h = (t1-t0)/n # step size\n","  # define arrays for T and Y\n","  T = [0]*(n+1)\n","  Y = [0]*(n+1)\n","  Y[0] = y\n","  T[0] = t0\n","\n","  for i in range(n):\n","    Y[i+1] = Y[i] + h*f(T[i],Y[i])\n","    T[i+1] = (i+1)*h\n","    #print(f\"At t = {T[i+1]:.1f}, y = {Y[i+1]:.4f}\")\n","  return T,Y;\n"],"metadata":{"id":"pVn5oPM-FF0Z"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# plot the predicted vs actual values for y\n","def plot_euler(T,Y,Y_act):\n","  plt.plot(T,Y,'o--',label='Approximate')\n","  #plt.plot(T,Y_act(T),label='Exact')\n","  plt.title('Eulers Method for dy/dt=f(t,y)')\n","  plt.xlabel('t')\n","  plt.ylabel('f(t)')\n","  plt.legend(loc='lower right')\n","  plt.show()"],"metadata":{"id":"_TUpQpRRFE-_"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# print and plot the error at the endpoint on a log-log plot \n","def calc_error(t0,t1,y0,Y_act):\n","  S = 6 \n","  endpoint = np.empty(S)\n","  h_vals = np.empty(S)\n","  n = 10\n","  for i in range(S):\n","    T,Y = euler1(t0,t1,y0,n)\n","    endpoint[i] = Y[-1]\n","    h_vals[i] = (t1-t0)/n\n","    n = n*2\n","  error = abs(Y_act(t1) - endpoint)\n","  plt.loglog(h_vals,error,label='error')\n","  plt.loglog(h_vals,h_vals,label='y=x')\n","  plt.loglog(h_vals,h_vals**2,label='y=x^2')\n","  plt.xlabel('h')\n","  plt.ylabel('error')\n","  plt.title('Order of convergence')\n","  plt.legend()\n","  plt.show()\n","  m,b = np.polyfit(np.log(h_vals), np.log(error), 1) # least squares to determine slope of the line\n","  print('The slope is ', m)\n","  print('The error at the end is:')\n","  for i in range(S):\n","    print('error = ', error[i], 'when h = ', h_vals[i])"],"metadata":{"id":"Gs63tCyKFHwY"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["\n","## Example 1 (2.4 \"Local and cumulative error\")\n","# Define the problem\n","n = 10; # number of steps\n","t0 = 0; # initial x\n","t1 = 1; # final x\n","y0 = 1; # initial y\n","\n","# Define right hand side f(t,y)\n","def f(t,y):\n","  return t+y;\n","\n","# calculate exact solution at grid points\n","Y_act = lambda T : 2*np.exp(T)-T-1\n","\n","'''\n","## Example 2 (2.4 Example 1)\n","n = 100; # number of steps\n","t0 = 0; # initial x\n","t1 = 5; # final x\n","y0 = -3; # initial y\n","\n","def f(t,y):\n","  return t + y/5;\n","\n","Y_act = lambda T : 22*np.exp(np.divide(T,5)) - np.multiply(T,5) - 25\n","'''\n","'''\n","## Example 3 (2.4 Example 4)\n","n = 100; # number of steps\n","t0 = 0; # initial x\n","t1 = 5; # final x\n","y0 = 1; # initial y\n","\n","def f(t,y):\n","  return y*np.cos(t);\n","\n","Y_act = lambda T : np.exp(np.sin(T))\n","'''\n","'''\n","## Example 3 (2.4 Example 4)\n","n = 100; # number of steps\n","t0 = 0; # initial x\n","t1 = 3; # final x\n","y0 = 1; # initial y\n","\n","def f(t,y):\n","  return -1 + 0.52*((1/y) - 1);\n"," '''"],"metadata":{"id":"CKhxLQ5hSrI_","colab":{"base_uri":"https://localhost:8080/","height":35},"executionInfo":{"status":"ok","timestamp":1667411392903,"user_tz":240,"elapsed":4,"user":{"displayName":"Mary Taranchuk","userId":"17926497385568558585"}},"outputId":"d261a453-5725-42b1-b32e-f755ad4b228d"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["'\\n## Example 3 (2.4 Example 4)\\nn = 100; # number of steps\\nt0 = 0; # initial x\\nt1 = 